@using CoreDepartman.Models
@model ICollection<CoreDepartman.Models.Personel>
<table class="table table-bordered">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Last Name</th>
        <th>City</th>
       
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.PerId</td>
            <td>@item.FirstName</td>
            <td>@item.LastName</td>
            <td>@item.City</td>
        </tr>
    }
</table>